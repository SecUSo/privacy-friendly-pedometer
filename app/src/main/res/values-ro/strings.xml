    <!-- HelpActivity -->
    <string name="activity_title_help">Ajutor</string>
    <string name="help_overview_heading">Informații despre aplicație</string>
    <string name="help_intro">Această aplicație poate să contorizeze în fundal numărul de pași, oferă un sumar despre cât ați mers și oferă posibilitatea definirii de notificări și de moduri de mers personalizate atunci când realizare obiectivului zilnic de pași este în pericol.</string>
    <string name="help_functionality_heading">Funcționalitate de bază</string>
    <string name="help_functionality_step_counter">Contor de pași</string>
    <string name="help_functionality_step_counter_summary">Contorul de pași numără pașii făcuți zilnic și poate fi dezactivat în setări.</string>
    <string name="help_functionality_notification_heading">Notificare permanentă</string>
    <string name="help_functionality_notification">Notificare</string>
    <string name="help_functionality_notification_summary">Notificarea permanentă este necesară pentru a putea menține aplicația de contorizare pornită în fundal. Conținutul notificării poate fi ales in setări.</string>
    <string name="help_functionality_notification_delete">Toate notificările se pot dezactiva în setările sistemului.</string>
    <string name="help_functionality_motivation_alert">Alertă motivațională</string>
    <string name="help_functionality_motivation_alert_summary">Aplicația o să arate alerta motivațională dacă numărul de pași făcuți în acea zi este in pericol să nu fie atins. Puteți defini criteriul, ora din zi si mesajul din notificare.</string>
    <string name="help_privacy_heading">Informații privind intimitatea</string>
    <string name="help_permission_boot_heading">Permisiune: Receive Boot Completed</string>
    <string name="help_permission_boot_description">este folosită pentru a porni contorul de pași și a seta notificarea la pornirea dispozitivului. Daca permisiunea nu este acceptată trebuie să porniți cel puțin o dată aplicația după start.</string>
    <string name="help_permissions_heading">Permisiuni</string>
    <string name="help_permissions_selection">Permisiunile folosite de Privacy Friendly Pedometer nu pot fi modificate de utilizator în nici o versiune de Android.</string>
    <string name="help_permission_wake_heading">Permisiune: Wake Lock</string>
    <string name="help_permission_wake_description">este folosit pentru a ține treaz procesorul și a putea contoriza în fundal pașii</string>

